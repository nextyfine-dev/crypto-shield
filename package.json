{
  "name": "crypto-shield",
  "version": "1.5.0",
  "description": "CryptoShield is a powerful and efficient Node.js library designed for seamless encryption and decryption using a variety of algorithms. Leveraging Node.js's built-in crypto module, this library empowers users with fine-grained control over their encryption processes, ensuring both security and flexibility.",
  "author": "nextyfine-dev",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "start": "node --watch dist/example.js",
    "build": "bun x tsc",
    "build:watch": "bun x tsc --watch",
    "test": "node  --test --experimental-test-coverage"
  },
  "devDependencies": {
    "@types/node": "^20.10.4"
  },
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "encryption",
    "decryption",
    "encrypt",
    "decrypt",
    "crypto",
    "encryption decryption",
    "aes encryption",
    "aes decryption",
    "file encryption",
    "file decryption",
    "text encryption",
    "text decryption",
    "cipher",
    "nodejs",
    "node encrypt decrypt",
    "crypto-shield",
    "crypto shield",
    "CryptoShield"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextyfine-dev/crypto-shield.git"
  },
  "bugs": {
    "url": "https://github.com/nextyfine-dev/crypto-shield/issues"
